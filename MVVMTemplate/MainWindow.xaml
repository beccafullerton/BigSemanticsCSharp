<Window x:Class="MVVMTemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="800" Width="1200">
    <Grid Background="#FFDADADA">
    	<TextBox x:Name="UrlBox" Height="26" Margin="8,8,135,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="14.667"/>
    	<Button x:Name="LoadButton" Content="Load Metadata" HorizontalAlignment="Right" Height="26" Margin="0,8,8,0" VerticalAlignment="Top" Width="114" Click="LoadButton_Click"/>
    	<Canvas x:Name="canvas" Margin="8,48,8,8" Background="White"/>
    </Grid>
</Window>
