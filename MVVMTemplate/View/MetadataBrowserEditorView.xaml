<UserControl x:Class="MVVMTemplate.View.MetadataBrowserEditorView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:View="clr-namespace:MVVMTemplate.View" mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="LayoutRoot">
        <Rectangle Fill="LightGray" Stroke="Black" RadiusX="10" RadiusY="10"/>
        <View:MetadataFieldsGroupView x:Name="FieldsView" Metadata="{Binding Path=Metadata}" />
    </Grid>
</UserControl>
